{
    "trailingComma": "none",
    "tabWidth": 2,
    "useTabs": true,
    "semi": false,
    "singleQuote": true,
    "jsxSingleQuote": true,
    "arrowParens": "avoid",
    "singleAttributePerLine": true,
    "importOrder": [
      "<THIRD_PARTY_MODULES>",
      "^@/components/(.*)$",
      "^@/layout/(.*)$",
      "^@/ui/(.*)$",
      "^@/utils/(.*)$",
      "^@/hooks/(.*)$",
      "^@/config/(.*)$",
      "^@/services/(.*)$",
      "^@/assets/(.*)$",
      "^@/providers/(.*)$",
      "^@/types/(.*)$",
      "^@/store/(.*)$",
      "^@/api/(.*)$",
      "^@../(.*)$",
      "^@./(.*)$",
      "(.scss)$"
    ],
    "importOrderSeparation": true,
    "importOrderSortSpecifiers": true,
    "plugins": ["@trivago/prettier-plugin-sort-imports"]
  }
  